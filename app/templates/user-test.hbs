<h1>All users</h1>
{{#each model as |user|}}
	<div class="well">
		<div class="form-group">
			<label>Name</label>
			{{input class="form-control" value=user.name}}
		</div>
		<div class="form-group">
			<label>Username</label>
			{{input class="form-control" value=user.username}}
		</div>
		<div class="form-group">
			<label>Income</label>
			{{input class="form-control" value=user.income}}
		</div>
		<div class="form-group">
			<label>Contribution</label>
			{{input class="form-control" value=user.contribution}}
		</div>
		<div class="form-group">
			<label style="color: red;">Percentage</label>
			{{input class="form-control" value=user.contributionPerc}}
		</div>
	
		<p>{{user.fullname}} contributes <span style="color: red;"> {{user.contributionPerc}}%</span></p>
	</div>
{{/each}}